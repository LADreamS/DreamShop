<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="LayoutItem">
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="textValue" format="string" />
        <attr name="textValueRight" format="string" />
        <attr name="leftImgBackground" format="reference|color" />
        <attr name="rightImgBackground" format="reference|color" />
        <attr name="centreImgBackground" format="reference|color" />
    </declare-styleable>

    <integer-array name="google_colors">
        <item>@color/red</item>
        <item>@color/blue</item>
        <item>@color/yellow</item>
        <item>@color/green</item>
    </integer-array>

    <!-- 充值金额数组-->
    <string-array name="pay_array">
        <item>@string/tv_money10</item>
        <item>@string/tv_money30</item>
        <item>@string/tv_money50</item>
        <item>@string/tv_money100</item>
        <item>@string/tv_money200</item>
        <item>@string/tv_money_other</item>
    </string-array>
</resources>
